<h3><%= Spree.t(:volume_prices) %></h3>
<table class="index">
  <thead>
    <tr>
      <th><%= Spree.t(:name) %></th>
      <th><%= Spree.t(:discount_type) %></th>
      <th><%= Spree.t(:range) %></th>
      <th><%= Spree.t(:amount) %></th>
      <th><%= Spree.t(:position) %></th>
      <th><%= Spree.t(:action) %></th>
    </tr>
  </thead>
  <tbody id="volume_prices">
    <%= f.fields_for :volume_prices do |vp_form| %>
      <%= render 'volume_price_fields', :f => vp_form %>
    <% end %>
  </tbody>
</table>
<%= link_to_add_fields Spree.t(:add_volume_price), "tbody#volume_prices" %>
<br/><br/>
